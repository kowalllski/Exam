<Window x:Class="Exam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exam"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="Frame" NavigationUIVisibility="Hidden" Source="/Exam;component/Pages/MainPage.xaml"/>
        <ListBox x:Name="TaskLists" HorizontalAlignment="Left" Height="110" VerticalAlignment="Top" Width="320" SelectionChanged="TaskLists_SelectionChanged"/>
        <Border x:Name="TaskListBorder" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Width="325">
        </Border>
        <Label x:Name="TaskListNameLabel" Content="Label" Margin="365,55,312,0" VerticalAlignment="Top"/>
        <CheckBox Content="CheckBox" Margin="365,98,162,0" Height="100" VerticalAlignment="Top"/>
        <Border x:Name="TasksBorder" BorderBrush="Black" BorderThickness="1" Margin="325,0,0,0">
        </Border>
    </Grid>
</Window>
